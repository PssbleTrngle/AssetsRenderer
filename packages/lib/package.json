{
   "name": "@pssbletrngle/assets-renderer",
   "version": "0.0.0-dev",
   "description": "Assets Renderer",
   "main": "dist/index.js",
   "exports": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "bin": {
      "render": "dist/cli/index.js"
   },
   "files": [
      "dist"
   ],
   "type": "module",
   "packageManager": "pnpm@7.9.5",
   "scripts": {
      "build": "tsc",
      "lint": "eslint src/**/*.ts",
      "render-acacia": "node dist/cli --output rendered --include *:acacia_*",
      "render-all": "node dist/cli --output rendered --cached-resources tmp --print-errors"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/PssbleTrngle/AssetsRenderer.git"
   },
   "publishConfig": {
      "registry": "https://npm.pkg.github.com"
   },
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/PssbleTrngle/AssetsRenderer/issues"
   },
   "homepage": "https://github.com/PssbleTrngle/AssetsRenderer#readme",
   "dependencies": {
      "@pssbletrngle/pack-resolver": "1.2.0",
      "@pssbletrngle/resource-merger": "1.2.4",
      "@types/command-line-usage": "^5.0.2",
      "@types/fs-extra": "^9.0.13",
      "@types/lodash-es": "^4.17.6",
      "@types/minimatch": "^3.0.5",
      "@types/node": "^17.0.45",
      "@types/three": "^0.146.0",
      "@types/tmp": "^0.2.3",
      "arg": "^5.0.2",
      "canvas": "^2.10.2",
      "chalk": "^5.0.1",
      "command-line-usage": "^6.1.3",
      "fs-extra": "^11.1.0",
      "lodash-es": "^4.17.21",
      "minimatch": "^5.1.0",
      "node-canvas-webgl": "^0.3.0",
      "three": "^0.147.0",
      "tmp": "^0.2.1"
   },
   "devDependencies": {
      "eslint": "^8.23.0",
      "eslint-config-custom": "workspace:*",
      "prettier": "^2.7.1",
      "ts-node": "^10.9.1",
      "tsconfig-config-custom": "workspace:*",
      "@types/node-canvas-webgl": "workspace:*",
      "typescript": "^4.8.2"
   }
