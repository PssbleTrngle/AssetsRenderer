{
   "name": "action",
   "scripts": {
      "lint": "eslint src/**/*.ts",
      "test": "jest",
      "build:main": "ncc build src/main.ts --out dist/main --source-map",
      "build:post": "ncc build src/post.ts --out dist/post --source-map",
      "build": "pnpm run build:main && pnpm run build:post"
   },
   "dependencies": {
      "@pssbletrngle/assets-renderer": "workspace:*"
   },
   "devDependencies": {
      "@actions/cache": "3.1.0-beta.2",
      "@actions/core": "^1.10.0",
      "@actions/github": "^5.1.1",
      "@actions/glob": "^0.3.0",
      "@types/jest": "^29.2.4",
      "@vercel/ncc": "^0.36.0",
      "eslint": "^8.23.0",
      "eslint-config-custom": "workspace:*",
      "jest": "^29.3.1",
      "ts-jest": "^29.0.3",
      "tsconfig-config-custom": "workspace:*",
      "typescript": "^4.8.2"
   }
}
